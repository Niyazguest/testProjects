<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <welcome-file-list>
      <welcome-file>TestSmartGWT.html</welcome-file>
  </welcome-file-list>  
   
  <servlet>
      <servlet-name>getReverseWordImpl</servlet-name>
      <servlet-class>ru.niyaz.testsmartgwt.server.GetReverseWordImpl</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
      <servlet-name>getReverseWordImpl</servlet-name>
      <url-pattern>/reverse/reverseWord</url-pattern>
  </servlet-mapping>

</web-app>
